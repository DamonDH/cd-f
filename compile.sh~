#!/bin/bash
# Program:
#    This program is to compile the currency detection source files,then 
# generate its execute file.
# History:
#  2015/05/20 22:18 Jin  First Release
PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin:~/bin
export PATH
g++ -o main ./src/main.cpp -I /usr/local/include -L /usr/local/lib -lopencv_core -lopencv_imgproc -lopencv_imgcodecs -lopencv_highgui -lopencv_ml -lopencv_features2d -lopencv_videoio 
